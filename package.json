{
  "name": "Protractor",
  "description": "Protractor end to end tests.",
  "version": "0.0.1",
  "repository": "https://github.com/Evilweed/protractor-async-typescript.git",
  "scripts": {
    "compile": "node_modules/.bin/tsc",
    "compile:watch": "node_modules/.bin/tsc --watch",
    "test": "node_modules/.bin/protractor ./build/specs/prot.conf.js",
    "test:debug": "node --inspect-brk node_modules/.bin/protractor ./build/specs/prot.conf.js",
    "webdriver:download:chromedriver": "./node_modules/protractor/bin/webdriver-manager update --standalone --versions.chrome 2.38",
    "webdriver:download:firefox": "./node_modules/protractor/bin/webdriver-manager update --standalone --versions.geckodriver 0.21"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "ts-node": "^4.1.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/lodash": "^4.14.110",
    "@types/node": "^8.0.0",
    "@types/request-promise-native": "^1.0.15",
    "protractor": "^5.3.2",
    "request-promise-native": "^1.0.5",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "typescript-eslint-parser": "^12.0.0"
  }
}
